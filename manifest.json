{
  "manifest_version": 2,
  "name": "DNS Gateway TEST",
  "version": "1.0.0",
  "description": "Test DNS Gateway functionality",
  
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  
  "browser_action": {
    "default_popup": "popup/popup.html"
  }
}